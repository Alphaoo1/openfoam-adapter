<?xml version="1.0"?>

<precice-configuration>

    <solver-interface dimensions="3">

    <!-- SWE solver variables -->
    <data:scalar name="Alpha"/>
    <data:scalar name="Gh"/>
    <data:vector name="Velocity"/>
    <!-- <data:vector name="VelocityGradient"/> -->


    <mesh name="SWE_solver-Mesh">
        <use-data name="Alpha"/>
        <use-data name="Gh"/>
        <use-data name="Velocity"/>
        <!-- <use-data name="VelocityGradient"/> -->
    </mesh>

    <mesh name="interFoam_solver_swe-of_subcritical-Mesh">
        <use-data name="Alpha"/>
        <use-data name="Gh"/>
        <use-data name="Velocity"/>
        <!-- <use-data name="VelocityGradient"/> -->

    </mesh>

    <participant name="SWE_solver">
        <use-mesh name="SWE_solver-Mesh" provide="yes"/>
        <use-mesh name="interFoam_solver_swe-of_subcritical-Mesh" from="interFoam_solver_swe-of_subcritical"/>

        <write-data name="Velocity" mesh="SWE_solver-Mesh"/>

        <read-data name="Alpha" mesh="SWE_solver-Mesh"/>
        <!-- <read-data name="VelocityGradient" mesh="SWE_solver-Mesh"/> -->

	    <mapping:nearest-neighbor direction="read" from="interFoam_solver_swe-of_subcritical-Mesh" to="SWE_solver-Mesh" constraint="consistent" />
    </participant>

    <participant name="interFoam_solver_swe-of_subcritical">
        <use-mesh name="interFoam_solver_swe-of_subcritical-Mesh" provide="yes"/>
        <use-mesh name="SWE_solver-Mesh" from="SWE_solver"/>

        <read-data name="Velocity" mesh="interFoam_solver_swe-of_subcritical-Mesh"/>

        <write-data name="Alpha" mesh="interFoam_solver_swe-of_subcritical-Mesh"/>
        <!-- <write-data name="VelocityGradient" mesh="interFoam_solver_swe-of_subcritical-Mesh"/> -->

	    <mapping:nearest-neighbor direction="read" from="SWE_solver-Mesh" to="interFoam_solver_swe-of_subcritical-Mesh" constraint="consistent" />
    </participant>

    <m2n:sockets from="SWE_solver" to="interFoam_solver_swe-of_subcritical" exchange-directory=".." />

    <coupling-scheme:serial-explicit>
        <!-- <time-window-size value="0.0009765625"/> -->
        <time-window-size value="0.125"/>

        <max-time value="10.0"/>
        <participants first="SWE_solver" second="interFoam_solver_swe-of_subcritical"/>
        <exchange data="Velocity" mesh="SWE_solver-Mesh" from="SWE_solver" to="interFoam_solver_swe-of_subcritical"/>
        <exchange data="Alpha" mesh="interFoam_solver_swe-of_subcritical-Mesh" from="interFoam_solver_swe-of_subcritical" to="SWE_solver"/>
        <!-- <exchange data="VelocityGradient" mesh="interFoam_solver_swe-of_subcritical-Mesh" from="interFoam_solver_swe-of_subcritical" to="SWE_solver"/> -->

    </coupling-scheme:serial-explicit>

    </solver-interface>
</precice-configuration>
