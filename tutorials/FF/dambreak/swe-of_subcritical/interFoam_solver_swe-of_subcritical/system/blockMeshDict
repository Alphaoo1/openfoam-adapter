// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 1;

xleft 10;
xobs1 15;
xobs2 17;
xright 20;

ybottom 0;
yobs 6.5;
ytop 10;

zback 0;
zmiddle 5;
zfront 10;

res 20;


vertices
(
    ($xleft $ybottom $zback) //0
    ($xobs1 $ybottom $zback)
    ($xobs2 $ybottom $zback)
    ($xright $ybottom $zback)

    ($xleft $yobs $zback) //4
    ($xobs1 $yobs $zback)
    ($xobs2 $yobs $zback)
    ($xright $yobs $zback)

    ($xleft $ytop $zback) //8
    ($xobs1 $ytop $zback)
    ($xobs2 $ytop $zback)
    ($xright $ytop $zback)

    ($xleft $ybottom $zfront) //12
    ($xobs1 $ybottom $zfront)
    ($xobs2 $ybottom $zfront)
    ($xright $ybottom $zfront)

    ($xleft $yobs $zfront) //16
    ($xobs1 $yobs $zfront)
    ($xobs2 $yobs $zfront)
    ($xright $yobs $zfront)

    ($xleft $ytop $zfront) //20
    ($xobs1 $ytop $zfront)
    ($xobs2 $ytop $zfront)
    ($xright $ytop $zfront)

);

blocks
(
    hex (0 1 5 4 12 13 17 16) (10 13 $res) simpleGrading (1 1 1)//1
    hex (2 3 7 6 14 15 19 18) (6 13 $res) simpleGrading (1 1 1)//2
    hex (4 5 9 8 16 17 21 20) (10 7 $res) simpleGrading (1 1 1)//3
    hex (5 6 10 9 17 18 22 21) (4 7 $res) simpleGrading (1 1 1)//4
    hex (6 7 11 10 18 19 23 22) (6 7 $res) simpleGrading (1 1 1)//5
);

edges
(
);

boundary
(
    leftWall_boundary
    {
        type wall;
        faces
        (
            (0 12 16 4)
            (4 16 20 8)
        );
    }
    rightWall
    {
        type wall;
        faces
        (
            (7 19 15 3)
            (11 23 19 7)
        );
    }
    lowerWall
    {
        type wall;
        faces
        (
            (0 1 13 12)
            (1 5 17 13)
            (5 6 18 17)
            (2 14 18 6)
            (2 3 15 14)
        );
    }
    atmosphere
    {
        type patch;
        faces
        (
            (8 20 21 9)
            (9 21 22 10)
            (10 22 23 11)
        );
    }
);

