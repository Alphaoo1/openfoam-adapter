FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 0
);

regions
(
    boxToCell
    {
        box (0 0 -500) (1000 500 500);
        fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }

    cylinderToCell
    {

            p1 (500 500 0);
            p2 (0 800 0);
            radius 250;


        fieldValues
        (
            volScalarFieldValue alpha.water 1
        );
    }
);


